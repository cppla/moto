{
  "log": {
    "level": "debug",
    "path": "./moto.log",
    "version": "1.0.1",
    "date": "2024-07-23"
  },
  "rules": [
    {
      "name": "正常模式",
      "listen": ":81",
      "mode": "normal",
      "prewarm": false,
      "timeout": 60000,
      "blacklist": null,
      "targets": [
        {
          "address": "www.baidu.com:80"
        },
        {
          "address": "www.baidu.com:80"
        }
      ]
    },
    {
      "name": "正则模式",
      "listen": ":82",
      "mode": "regex",
      "prewarm": false,
      "timeout": 60000,
      "blacklist": null,
      "targets": [
        {
          "regexp": "^(GET|POST|HEAD|DELETE|PUT|CONNECT|OPTIONS|TRACE)",
          "address": "www.baidu.com:80"
        },
        {
          "regexp": "^SSH",
          "address": "www.baidu.com:22"
        }
      ]
    },
    {
      "name": "智能加速",
      "listen": ":83",
      "mode": "boost",
      "prewarm": true,
      "timeout": 60000,
      "blacklist": null,
      "targets": [
        {
          "address": "www.baidu.com:80"
        },
        {
          "address": "www.baidu.com:80"
        }
      ]
    },
    {
      "name": "轮询模式",
      "listen": ":84",
      "mode": "roundrobin",
      "prewarm": true,
      "timeout": 60000,
      "blacklist": null,
      "targets": [
        {
          "address": "www.baidu.com:80"
        },
        {
          "address": "www.baidu.com:80"
        }
      ]
    }
  ]
}

{
  "log": {
    "level": "debug",
    "path": "./moto.log",
    "version": "1.0.1",
    "date": "2024-07-23"
  },
  "wafs": [
    {
      "name": "限制单位时间内总请求次数@threshold",
      "blackcountry": [
        "US"
      ],
      "threshold": 200,
      "findtime": 30,
      "bantime": 86400
    },
    {
      "name": "限制单位时间内总数据量@threshold",
      "blackcountry": [
        "TW",
        "US"
      ],
      "threshold": 10240,
      "findtime": 30,
      "bantime": 86400
    }
  ],
  "rules": [
    {
      "name": "正常模式",
      "listen": ":81",
      "mode": "normal",
      "timeout": 3000,
      "blacklist": null,
      "targets": [
        {
          "address": "1.1.1.1:85"
        },
        {
          "address": "2.2.2.2:85"
        }
      ]
    },
    {
      "name": "正则模式",
      "listen": ":82",
      "mode": "regex",
      "timeout": 3000,
      "blacklist": null,
      "targets": [
        {
          "regexp": "^(GET|POST|HEAD|DELETE|PUT|CONNECT|OPTIONS|TRACE)",
          "address": "1.1.1.1:80"
        },
        {
          "regexp": "^SSH",
          "address": "2.2.2.2:22"
        }
      ]
    },
    {
      "name": "智能加速",
      "listen": ":83",
      "mode": "boost",
      "timeout": 300,
      "blacklist": null,
      "targets": [
        {
          "address": "1.1.1.1:85"
        },
        {
          "address": "2.2.2.2:85"
        }
      ]
    },
    {
      "name": "轮询模式",
      "listen": ":84",
      "mode": "roundrobin",
      "timeout": 300,
      "blacklist": null,
      "targets": [
        {
          "address": "1.1.1.1:85"
        },
        {
          "address": "2.2.2.2:85"
        }
      ]
    }
  ]
  ,
  "accelerator": {
    "enabled": false,
    "role": "client",
    "remote": "127.0.0.1:9900",
    "listen": ":9900",
    "tunnels": 0,
    "duplication": 0,
    "frameSize": 8192
  },
  "lossAdaptation": {
    "enabled": true,
    "windowSeconds": 10,
    "probeIntervalMs": 500,
    "rules": [
      {"lossBelow": 1,  "dup": 1},
      {"lossBelow": 10, "dup": 2},
      {"lossBelow": 20, "dup": 3},
      {"lossBelow": 30, "dup": 4},
      {"lossBelow": 101, "dup": 5}
    ]
  }
}
